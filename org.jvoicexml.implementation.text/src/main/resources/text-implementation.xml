<?xml version="1.0" encoding="UTF-8"?>
<implementation xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="jvxml-implementation-0-7.xsd">
    <repository>text</repository>
    <classpath>dist/org.jvoicexml.implementation.text.jar</classpath>
    <classpath>dist/org.jvoicexml.client.text.jar</classpath>
    <classpath>dist/org.jvoicexml.srgs.jar</classpath>
    <!-- classpath>dist/org.jvoicexml.grammar.interpreter.luis.jar</classpath-->
    <classpath>3rdparty/protobuf3.0.0/lib/protobuf-java-3.0.0.jar</classpath>

    <beans:bean class="org.jvoicexml.implementation.text.TextPlatformFactory">
        <beans:property name="instances" value="1" />
        <beans:property name="grammarParsers">
            <beans:list value-type="org.jvoicexml.implementation.grammar.GrammarParser">
                <beans:bean class="org.jvoicexml.srgs.SrgsSisrXmlGrammarParser" />
                <!-- bean class="org.jvoicexml.interpreter.grammar.luis.LUISGrammarParser">
                    <property name="subscription" value="MUST-BE-SUPPLIED-VALUE" />
                </bean-->
            </beans:list>
        </beans:property>
    </beans:bean>
</implementation>
